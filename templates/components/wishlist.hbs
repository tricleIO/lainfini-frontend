<div class="wishlist">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 text-center">
                {{> "parts/ui/heading" title="your wishlist" type="h3" line='1'}}
            </div>
        </div>
        <div class="row wishlist-product-list text-center animated fadeInUp">
            <div class="col-12 col-sm-6 col-lg-6 col-xl-3">
                <div class="wish-product">
                    <div class="wish-product__background">
                        <img class="img-fluid" src="../../userfiles/arrival-1.png" alt="product img">
                    </div>
                    <div class="wish-product__content">
                        <div class="wish-product__title">
                            <h4>Blue moon</h4>
                            <i class="icon icon-shop"></i>
                        </div>
                        <div class="wish-product__price">
                            <span>$239.00</span>
                        </div>
                    </div>
                    {{> "parts/social_nav"}}
                </div>
            </div>
            <div class="col-12 col-sm-6 col-lg-6 col-xl-3">
